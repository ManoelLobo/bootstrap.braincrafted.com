{{ "{{ knp_menu_render('BcBootstrapDemoBundle:Builder:basicNavbar', {'nav_type': 'navbar', 'pull': 'left'}) }}" }}
{{ "{{ knp_menu_render('BcBootstrapDemoBundle:Builder:basicNavbar', {'nav_type': 'navbar', 'pull': 'right'}) }}" }}
</pre>

